<Window x:Class="SillyRabbit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SillyRabbit"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnSerialize" Content="Serialize" HorizontalAlignment="Left" Margin="104,90,0,0" VerticalAlignment="Top" Width="75" Click="BtnSerialize_Click"/>
        <Button x:Name="btnDeSerialize" Content="Deserialize" HorizontalAlignment="Left" Margin="104,115,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="tbOutput" HorizontalAlignment="Left" Margin="227,90,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="104" Width="166"/>

    </Grid>
</Window>
